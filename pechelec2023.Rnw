# Fonction de calcul du recrutement fluvial en fonction d'hypothèsese de survie
#' parametre mortpas => mortalité des civelles sur la passe
#' parametre mortxp => mortalité des civelles transportées 
fnrec <- function(mortpas, mortxp, mortecl, data, type = "n") {
	# In the following we assume the same mortality rate of glass eel from the
	# trapping ladder and of those coming form the sluice
	if (type == "n") {
		recflu <- rowSums(cbind(data$passe_relachees_n * (1 - mortpas), 
						data$transport_total_n *  (1 - mortxp), 
						data$peche_amont_n * (1 - mortxp), 
						data$transport_CLPMEM_n * (1 - mortxp), 
						data$eclusage_n * (1 - mortecl)), na.rm = TRUE)
		return(recflu)
	} else if (type == "kg") {
		recflu <- rowSums(cbind(data$passe_relachees_kg * (1 - mortpas),
						data$transport_total_kg * (1 - mortxp), 
						data$peche_amont_kg * (1 - mortxp), 
						data$transport_CLPMEM_kg * (1 - mortxp), 
						data$eclusage_kg * (1 - mortecl)), na.rm = TRUE)
	}
	return(recflu)
}
